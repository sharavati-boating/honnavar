<script>
(function () {
    const params = new URLSearchParams(window.location.search);

    // Extract and decode values safely
    const catRaw = params.get('cat') || '';
    const urlRaw = params.get('url') || '';

    let cat = '';
    let url = '';

    try {
        cat = decodeURIComponent(catRaw.replace(/\+/g, ' '));
    } catch (e) {
        cat = catRaw;
    }

    try {
        url = decodeURIComponent(urlRaw);
    } catch (e) {
        url = urlRaw;
    }

    // Display values if elements exist
    const catElem = document.getElementById('categoryValue');
    if (catElem) catElem.textContent = cat;

    const urlElem = document.getElementById('triedToVisit');
    if (urlElem) urlElem.textContent = url;
})();
</script>



